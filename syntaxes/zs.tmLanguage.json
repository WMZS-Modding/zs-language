{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "ZS",
  "scopeName": "source.zs",
  "patterns": [
    { "include": "#keywords" },
    { "include": "#strings" },
    { "include": "#comments" },
    { "include": "#block-comments" },
    { "include": "#numbers" },
    { "include": "#class-name" },
    { "include": "#invalidGrammar" },
    { "include": "#true-false"},
    { "include": "#function-noun" },
    { "include": "#script-noun" }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.zs",
          "match": "\\b([Ww][Hh][Ee][Nn]|[Ii][Ff]|[Tt][Hh][Ee][Nn]|[Ee][Ll][Ss][Ee]|[Ww][Ii][Tt][Hh]|[Ww][Hh][Ii][Ll][Ee]|[Dd][Oo][Nn][Ee]|[Aa][Nn][Dd]|[Tt][Rr][Ii][Gg][Gg][Ee][Rr]|[Ee][Xx][Cc][Ee][Pp][Tt]|[Oo][Nn][Cc][Ee]|[Tt][Ww][Ii][Cc][Ee]|[Ss][Ee][Tt])\\b"
        },
        {
          "name": "entity.name.type.zs",
          "match": "\\b([Pp][Uu][Bb][Ll][Ii][Cc]|[Pp][Rr][Ii][Vv][Aa][Tt][Ee])\\b"
        },
        {
          "name": "variable.language.zs",
          "match": "\\b([Ii][Tt][Ss][Ee][Ll][Ff]|[Mm]ono[Bb]ehaviour|[Uu]nity[Ss]ystem|[Uu][Ii][Ee]vent|[Uu]nity[Ee]ngine|[Ii][Tt]|[Tt][Hh][Ee][Yy]|[Tt][Hh][Ee][Mm]|[Tt][Hh][Ee][Mm][Ss][Ee][Ll][Ff])\\b"
        },
        {
          "name": "invalid.illegal.zs",
          "match": "\\b([Aa][Mm]|[Ww][Aa][Ss]|[Ww][Ee][Rr][Ee]|[Hh][Aa][Dd]|[Hh][Aa][Ss]|[Hh][Aa][Vv][Ee]|[Mm][Aa][Yy]|[Mm][Ii][Gg][Hh][Tt]|[Mm][Uu][Ss][Tt]|[Ss][Hh][Aa][Ll][Ll]|[Ss][Hh][Oo][Uu][Ll][Dd]|[Ww][Oo][Uu][Ll][Dd]|[Cc][Oo][Uu][Ll][Dd]|[Dd][Ii][Dd]|[Bb][Ee][Ii][Nn][Gg]|[Bb][Ee][Ee][Nn]|[Ff][Ee][Ee][Ll][Ss]|[Ll][Oo][Vv][Ee][Ss]?|[Hh][Aa][Tt][Ee][Ss]?|[Dd][Ii][Ss][Ll][Ii][Kk][Ee][Ss]?|[Pp][Rr][Ee][Ff][Ee][Rr]|[Ww][Ii][Ss][Hh]|[Ww][Aa][Nn][Tt]|[Nn][Ee][Ee][Dd]|[Kk][Nn][Oo][Ww]|[Uu][Nn][Dd][Ee][Rr][Ss][Tt][Aa][Nn][Dd]|[Bb][Ee][Ll][Ii][Ee][Vv][Ee]|[Tt][Hh][Ii][Nn][Kk]|[Rr][Ee][Mm][Ee][Mm][Bb][Ee][Rr]|[Ff][Oo][Rr][Gg][Ee][Tt]|[Oo][Ww][Nn]|[Pp][Oo][Ss][Ss][Ee][Ss][Ss]|[Bb][Ee][Ll][Oo][Nn][Gg]|[Ss][Ee][Ee]|[Hh][Ee][Aa][Rr]|[Ss][Mm][Ee][Ll][Ll]|[Tt][Aa][Ss][Tt][Ee]|[Ff][Ee][Ee][Ll]|[Bb][Ee]|[Ss][Ee][Ee][Mm]|[Aa][Pp][Pp][Ee][Aa][Rr]|[Ll][Oo][Oo][Kk]|[Dd][Ii][Ss][Aa][Pp][Pp][Ee][Aa][Rr]|[Cc][Aa][Rr][Ee]|[Bb][Ee][Cc][Oo][Mm][Ee]|[Gg][Ee][Tt]|[Gg][Rr][Oo][Ww]|[Ss][Tt][Aa][Yy]|[Rr][Ee][Mm][Aa][Ii][Nn])\\b"
        },
        {
          "name": "markup.inserted.zs",
          "match": "\\b([Mm][Oo][Vv][Ee]|[Rr][Uu][Nn]|[Pp][Ll][Aa][Yy]|[Tt][Oo][Uu][Cc][Hh]|[Cc][Uu][Tt]|[Ww][Aa][Ii][Tt]|[Dd][Oo][Ee]?[Ss]?|[Bb][Ll][Oo][Ww]|[Ii][Ss]|[Aa][Rr][Ee]|[Ff][Ll][Oo][Ww]|[Ll][Oo][Aa][Dd]|[Cc][Aa][Ll][Ll]|[Ss][Tt][Oo][Pp]|[Dd][Ee][Ll][Ee][Tt][Ee]|[Dd][Ee][Ss][Tt][Rr][Oo][Yy]|[Rr][Ee][Mm][Oo][Vv][Ee]|[Rr][Ee][Pp][Ll][Aa][Cc][Ee])\\b"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.zs",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.zs",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "comments": {
      "patterns": [
        {
          "name": "comment.line.slashdash.zs",
          "begin": "-/",
          "end": "$"
        }
      ]
    },
    "block-comments": {
      "patterns": [
        {
          "name": "comment.block.zs",
          "begin": "\\*/-",
          "end": "/-\\*",
          "patterns": [
            {
              "match": ".",
              "name": "comment.block.content.zs"
            }
          ]
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "support.function.zs",
          "match": "\\b\\d+(\\.\\d+)?\\b"
        }
      ]
    },
    "class-name": {
      "patterns": [
        {
          "name": "meta.class.zs",
          "begin": "\\b([Cc][Ll][Aa][Ss][Ss]|[Pp][Rr][Ee][Ff][Aa][Bb])\\b\\s+",
          "beginCaptures": {
            "1": {
              "name": "entity.name.type.class.zs"
            }
          },
          "end": "(?=\\{|:|\\s|$)",
          "patterns": [
            {
              "match": "[A-Za-z_][A-Za-z0-9_]*",
              "name": "keyword.declaration.zs"
            }
          ]
        }
      ]
    },
    "invalidGrammar": {
      "patterns": [
        {
          "name": "invalid.illegal.zs",
          "match": "\\b([Ii]|[Yy][Oo][Uu]|[Hh][Ee]|[Ss][Hh][Ee]|[Ww][Ee]|[Uu][Ss]|[Mm][Ee]|[Hh][Ii][Mm]|[Hh][Ee][Rr]|[Oo][Uu][Rr][Ss]|[Hh][Ii][Ss]|[Hh][Ee][Rr][Ss]|[Yy][Oo][Uu][Rr]|[Mm][Yy]|[Mm][Ii][Nn][Ee]|[Oo][Uu][Rr]|[Tt][Hh][Ee][Ii][Rr]|[Ii][Tt][Ss]|[Tt][Hh][Ee][Ii][Rr][Ss]|[Mm][Yy][Ss][Ee][Ll][Ff]|[Yy][Oo][Uu][Rr][Ss][Ee][Ll][Ff]|[Hh][Ii][Mm][Ss][Ee][Ll][Ff]|[Hh][Ee][Rr][Ss][Ee][Ll][Ff]|[Oo][Uu][Rr][Ss][Ee][Ll][Ff]|[Oo][Uu][Rr][Ss][Ee][Ll][Vv][Ee][Ss]|[Yy][Oo][Uu][Rr][Ss][Ee][Ll][Vv][Ee][Ss]|[Mm][Yy][Ss][Ee][Ll][Vv][Ee][Ss]|[Hh][Ii][Mm][Ss][Ee][Ll][Vv][Ee][Ss]|[Hh][Ee][Rr][Ss][Ee][Ll][Vv][Ee][Ss]|[Tt][Hh][Ee][Mm][Ss][Ee][Ll][Vv][Ee][Ss]|[Ii][Tt][Ss][Ee][Ll][Vv][Ee][Ss])\\b"
        }
      ]
    },
    "true-false": {
      "patterns": [
        {
          "name": "invalid.illegal.zs",
          "match": "\\b([Ff][Aa][Ll][Ss][Ee])\\b"
        },
        {
          "name": "markup.inserted.zs",
          "match": "\\b([Tt][Rr][Uu][Ee])\\b"
        }
      ]
    },
    "function-noun": {
      "patterns": [
        {
          "name": "support.function.noun.zs",
          "match": "<[^<>\\n]+>"
        }
      ]
    },
    "script-noun": {
      "patterns": [
        {
          "name": "support.function.script.zs",
          "match": "\\([^()\\n]+\\)"
        }
      ]
    }
  },
  "uuid": "4d1eb976-65c1-4039-bbc1-97b7eb5d7b2a"
}
